  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#erythrinaPoeppigiana">
    <ns2:commonName xml:lang="es">PÍSAMO</ns2:commonName>
    <ns2:shortDescription xml:lang="es">Árbol grande que puede alcanzar hasta 35 ms de altura. Presenta aguijones gruesos. Las flores son de color 
    rojo-anaranjado y se encuentran en racimos erectos. Los frutos son legumbres de hasta 25 cms de largo. Produce gran 
    cantidad de follaje que sirve como abono en cultivos, regenera suelos al fijar nitrógeno.</ns2:shortDescription>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns5:sameAs rdf:resource="https://www.wikidata.org/wiki/Q5837321"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/647292"/>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf"/>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/416279"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/vYig5sj.jpg"/>
    <ns2:scientificName xml:lang="la">Erythrina poeppigiana</ns2:scientificName>
    <ns3:isPrimaryTopicOf rdf:resource="http://aprendeenlinea.udea.edu.co/ova/?q=node/455"/>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/8311514"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#oreopanaxFloribundum">
