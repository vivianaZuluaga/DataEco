    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns2:commonName xml:lang="es">CHAGUALO</ns2:commonName>
    <ns4:category xml:lang="es">FLORA</ns4:category>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#psidiumGuineense">
