    <ns2:scientificName xml:lang="la">Pithecellobium lanceolatum</ns2:scientificName>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns3:depiction rdf:resource="http://i.imgur.com/I3dpH20.jpg"/>
    <ns2:commonName xml:lang="es">ESPINO DE MONO</ns2:commonName>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#guaduaAngustifoliaBicolor">
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns3:depiction rdf:resource="http://i.imgur.com/D5VIXzt.jpg"/>
    <ns5:sameAs rdf:resource="https://www.wikidata.org/wiki/Q1491422"/>
    <ns2:scientificName xml:lang="la">Guadua angustifolia bicolor</ns2:scientificName>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/4155046"/>
    <ns2:shortDescription xml:lang="es">Bambú gigante, espinoso, con culmos erectos y huevos que alcanzan alturas hasta de 25 ms y diámetros entre 10-25 
    cms. Sus entrenudos tienen paredes hasta de 2 cms de espesor. Se diferencia de la especie tipo por tener rayas 
    longitudinales amarillas sobre el culmo verde. Tiene gran potencial para la fabricación de artesanías.</ns2:shortDescription>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf"/>
