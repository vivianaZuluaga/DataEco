  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#petiveriaAlliacea">
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf"/>
    <ns2:shortDescription xml:lang="es">Planta herbácea perenne, crece hasta aproximadamente 1 m de altura. En algunos lugares se le conoce como 
    "yerba de ajo" debido al fuerte olor a ajo que emana, especialmente de las raíces. La raíz posee propiedades 
    anestésicas y analgésicas y se considera más poderosa que las hojas.</ns2:shortDescription>
    <ns2:commonName xml:lang="es">ANAMÚ</ns2:commonName>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/3084012"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/46142"/>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/594860"/>
