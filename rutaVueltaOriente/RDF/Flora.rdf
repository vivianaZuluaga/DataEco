  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#acaciaFarnesiana">
    <ns3:depiction rdf:resource="http://i.imgur.com/WqonGiJ.jpg"/>
