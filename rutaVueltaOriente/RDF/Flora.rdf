    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/681502"/>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#miconia">
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns2:shortDescription xml:lang="es">Arbusto con copa redondeada, hojas simples opuestas, el borde es finamente aserrado, haz verde oscuro. 
    Inflorescencias muy tupidas en panículas terminales, de color verde pálido. Las flores son pequeñas y amarillentas, 
    blancas o rosadas. El fruto es una baya de color rojo al madurar. Se utiliza como leña.</ns2:shortDescription>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/3188585"/>
    <ns2:commonName xml:lang="es">NIGUITO</ns2:commonName>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/60394"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/703ZLjF.jpg"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/263288"/>
    <ns2:scientificName xml:lang="la">Miconia sp</ns2:scientificName>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns5:sameAs rdf:resource="http://dbpedia.org/resource/Miconia"/>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf"/>
    <ns3:isPrimaryTopicOf rdf:resource="http://www.biodiversidad.co/fichas/4918"/>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#tessariaIntegrifolia">
