  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#escobediaScabrifolia">
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns2:shortDescription xml:lang="es">Hierba perenne, erecta, que alcanza un tamaño de 40–100 cms de alto, glabra; tallo simple. Hojas opuestas. Flores 
    solitarias en las axilas de las hojas. Uso de sus raíces anaranjadas como colorante natural de los alimentos y uso 
    en la medicina popular; se ha comprobado que las flores tienen sustancias antibacteriales.</ns2:shortDescription>
    <ns3:depiction rdf:resource="http://i.imgur.com/zQoark4.jpg"/>
    <ns3:isPrimaryTopicOf rdf:resource="http://agrodiversos.blogspot.com.co/p/blog-page.html?view=timeslide"/>
    <ns2:commonName xml:lang="es">AZAFRÁN DE RAÍZ</ns2:commonName>
