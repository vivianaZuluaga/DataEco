    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/7333251"/>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf"/>
    <ns5:sameAs rdf:resource="https://www.wikidata.org/wiki/Q21399372"/>
