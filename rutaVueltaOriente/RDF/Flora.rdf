    <ns3:isPrimaryTopicOf rdf:resource="http://www.biodiversidad.co/fichas/2197"/>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/29342"/>
    <ns2:scientificName xml:lang="la">Philodendron spp</ns2:scientificName>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaVueltaAOriente/Flora.rdf#solanumTorvum">
    <ns2:commonName xml:lang="es">FRIEGAPLATOS</ns2:commonName>
    <ns3:isPrimaryTopicOf rdf:resource="http://www.conabio.gob.mx/malezasdemexico/solanaceae/solanum-torvum/fichas/ficha.htm"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/n9zVGBJ.jpg"/>
    <ns2:shortDescription xml:lang="es">Arbusto de hasta 2.5 ms de altura, cubierto de pelos estrellados (con las ramas libres, es decir unidas 
    únicamente en la base), armado de espinas dispersas rectas o algunas recurvadas. Hojas escasamente lobadas, 
    cubiertas con pelos estrellados; flores blancas. Frutos globosos, verdes, sin pelillos.</ns2:shortDescription>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns2:scientificName xml:lang="la">Solanum torvum</ns2:scientificName>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/119830"/>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
