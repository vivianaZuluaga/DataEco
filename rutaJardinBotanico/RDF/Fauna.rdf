  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaDelJardinBotanico/Fauna.rdf#crematogasterCarinata">
    <ns3:isPrimaryTopicOf rdf:resource="http://www.antwiki.org/wiki/Crematogaster_carinata"/>
    <ns2:kingdomName xml:lang="es">Animal</ns2:kingdomName>
    <ns2:scientificName xml:lang="la">Crematogaster carinata</ns2:scientificName>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/1324354"/>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/407090"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/1720225"/>
    <ns2:shortDescription xml:lang="es">Pequeña e inofensiva que a menudo coloniza ramas de árboles. Parece ser capaz de mantener las colonias sin salir de 
  su árbol de origen.</ns2:shortDescription>
    <ns4:category xml:lang="es">FAUNA</ns4:category>
    <ns5:sameAs rdf:resource="http://dbpedia.org/resource/Crematogaster_carinata"/>
