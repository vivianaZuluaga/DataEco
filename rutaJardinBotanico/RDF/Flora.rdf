    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/58168"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/lXk87cJ.jpg"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
