    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/145709"/>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaDelJardinBotanico/Flora.rdf#attaleaNucifera">
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/2732819"/>
    <ns5:sameAs rdf:resource="https://www.wikidata.org/wiki/Q15457188"/>
    <ns2:scientificName xml:lang="la">Attalea nucifera</ns2:scientificName>
    <ns2:shortDescription xml:lang="es">Palma de tallo subterráneo y grandes hojas de hasta 8 ms de largo, sus frutos miden de 5-7 cms de largo y son de 
    color café, de sabor parecido al coco. Es exclusiva de los bosques húmedos del Magdalena medio en Colombia y se 
    encuentra en peligro de extinción por la destrucción de su hábitat.</ns2:shortDescription>
    <ns3:depiction rdf:resource="http://i.imgur.com/qxni3iT.jpg"/>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaDelJardinBotanico/Flora.rdf"/>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/1131736"/>
    <ns3:isPrimaryTopicOf rdf:resource="https://en.wikipedia.org/wiki/Attalea_(palm)"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/115444"/>
    <ns2:commonName xml:lang="es">ALMENDRÓN</ns2:commonName>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaDelJardinBotanico/Flora.rdf#dypsisLutescens">
    <ns2:shortDescription xml:lang="es">Esta palma tiene un tallo cilíndrico de color gris, que alcanza los 5 ms de altura y los 15 cms de diámetro. Su 
    fruto es una baya de color amarillo oscuro a negro que mide hasta 1,5 cms de largo. Además sirve de alimento para la 
    avifauna y para la apicultura ya que es una especie melífera.</ns2:shortDescription>
    <ns3:depiction rdf:resource="http://i.imgur.com/mXJPnm1.jpg"/>
    <ns3:isPrimaryTopicOf rdf:resource="http://www.biodiversidad.co/fichas/1297"/>
    <ns2:commonName xml:lang="es">PALMA ARECA</ns2:commonName>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/1095187"/>
    <ns2:scientificName xml:lang="la">Dypsis lutescens</ns2:scientificName>
    <ns5:sameAs rdf:resource="http://dbpedia.org/resource/Dypsis_lutescens"/>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/2735935"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/131267"/>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaDelJardinBotanico/Flora.rdf"/>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
