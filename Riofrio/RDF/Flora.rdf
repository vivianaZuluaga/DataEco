    <ns2:commonName xml:lang="es">CASCARILLA</ns2:commonName>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/Riofrio/Flora.rdf#pictetiaAculeata">
