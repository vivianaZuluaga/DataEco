    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaDelMaiz/Flora.rdf"/>
    <ns2:shortDescription xml:lang="es">Planta epífita, con pseudobulbos de largo variable que llevan a su extremidad una o a dos hojas sin pecíolo. 
    Flores vistosas que se desarrollan en el tallo.</ns2:shortDescription>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
