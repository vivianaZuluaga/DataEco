    <rdfs:seeAlso rdf:resource="http://eol.org/pages/1084926"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/bziWfgl.jpg"/>
    <ns2:commonName xml:lang="es">AJO</ns2:commonName>
    <ns2:scientificName xml:lang="la">Allium Sativum</ns2:scientificName>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns2:shortDescription xml:lang="es">Alcanza entre 30 y 40 cms de altura, sus hojas son macizas, radicales y largas. Sus flores son blancas y 
    rosadas y cada una puede presentar 6 pétalos, 6 estambres y un pistilo. Su raíz es bulbosa, compuesta de 6 a 12 
    bulbillos ("dientes de ajo").</ns2:shortDescription>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaDelMaiz/Flora.rdf#tabebuiaGuayacan">
