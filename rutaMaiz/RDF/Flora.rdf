    <ns2:commonName xml:lang="es">SÁBILA</ns2:commonName>
    <ns2:scientificName xml:lang="la">Aloe Vera</ns2:scientificName>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/1085598"/>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns3:isPrimaryTopicOf rdf:resource="https://en.wikipedia.org/wiki/Aloe_vera"/>
