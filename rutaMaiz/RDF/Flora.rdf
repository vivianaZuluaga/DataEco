    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns3:isPrimaryTopicOf rdf:resource="https://en.wikipedia.org/wiki/Guadua_angustifolia"/>
    <ns2:scientificName xml:lang="la">Guadua Angustifolia</ns2:scientificName>
    <ns2:shortDescription xml:lang="es">La raíz es un rizoma que almacena los nutrimentos, estos rizomas producen raicillas adventicias, el tallo de 
    forma cilíndrica y con nudos, ramas solitarias y muy espinosas en los nudos basales y con presencia o no de hojas; 
    hojas lanceoladas verdes lustrosas.</ns2:shortDescription>
    <ns5:sameAs rdf:resource="http://dbpedia.org/resource/Guadua"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/323898"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/WLLO9uD.jpg"/>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/5800278"/>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/4155017"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaDelMaiz/Flora.rdf"/>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaDelMaiz/Flora.rdf#mangiferaIndica">
    <ns2:scientificName xml:lang="la">Mangifera Indica</ns2:scientificName>
    <ns1:isAbout rdf:resource="http://www.dataeco.org/dataseteco/RutaDelMaiz/Flora.rdf"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/29780"/>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/3190638"/>
    <ns2:shortDescription xml:lang="es">El mango típico constituye un árbol de tamaño mediano, de 10-30 ms de altura. El tronco es más o menos recto, 
    cilíndrico y de 75-100 cms de diámetro, cuya corteza de color gris - café tiene grietas.</ns2:shortDescription>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/582270"/>
    <ns2:commonName xml:lang="es">MANGO</ns2:commonName>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
    <ns3:isPrimaryTopicOf rdf:resource="http://www.empresario.com.co/recursos/page_flip/MEGA/mega_mango/files/ficha%20mango.pdf"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/GtxlZkC.jpg"/>
    <ns5:sameAs rdf:resource="http://dbpedia.org/resource/Mangifera_indica"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
  </ns2:TaxonName>
