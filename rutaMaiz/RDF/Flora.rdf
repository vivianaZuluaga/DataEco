    <ns3:isPrimaryTopicOf rdf:resource="http://vida-flores.blogspot.com.co/"/>
    <ns2:commonName xml:lang="es">MAGNOLIA</ns2:commonName>
    <rdfs:seeAlso rdf:resource="http://eol.org/pages/1154991"/>
    <rdfs:seeAlso rdf:resource="http://www.gbif.org/species/3153283"/>
    <ns5:sameAs rdf:resource="http://www.uniprot.org/taxonomy/3406"/>
    <ns2:scientificName xml:lang="la">Magnolia Grandiflora L.</ns2:scientificName>
    <ns5:sameAs rdf:resource="http://dbpedia.org/resource/Magnolia_grandiflora"/>
    <ns3:depiction rdf:resource="http://i.imgur.com/p3tLqzX.jpg"/>
    <ns4:category xml:lang="es">FLORA</ns4:category>
    <ns2:shortDescription xml:lang="es">Es un árbol de tamaño medio a grande de 20-30 ms de altura. Las hojas son perennes, simples o ampliamente 
    ovadas de 12-20 cms de longitud y 6-12 cms de ancho con los márgenes dentados, son de color verde oscuro y 
    se vuelven color marrón cuando llega el invierno.</ns2:shortDescription>
    <ns2:kingdomName xml:lang="es">Vegetal</ns2:kingdomName>
  </ns2:TaxonName>
  <ns2:TaxonName rdf:about="http://www.dataeco.org/dataseteco/RutaDelMaiz/Flora.rdf#guaduaAngustifolia">
